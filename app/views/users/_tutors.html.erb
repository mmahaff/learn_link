<div class="row">
  <h1>Connect with tutors near you</h1>
  <% @users.each do |user| %>
  <div class="small-12 medium-4 columns profile-card">
      <div class="small-12 columns">
        <%= image_tag user.profile_photo.medium_profile_picture, class: 'profile-list-image' %>
      </div>
      <div class="small-12 columns">
        <h4><%= link_to user.first_name, user_path(user)%></h4>
        <p><%= user.city %>, <%= user.state%></p>
        <p><%= user.location %></p>
      </div>
  </div>
  <% end %>
</div>
